{
    "name": "celysium/helper",
    "description": "The package contains the most commonly used helper function",
    "keywords": [
        "Laravel",
        "helper",
        "sructure",
        "repository"
    ],
    "autoload": {
        "psr-4": {
            "Celysium\\Helper\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "authors": [
        {
            "name": "Nasser Hekmati",
            "email": "hekmati.nasser@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Celysium\\Helper\\HelperServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
